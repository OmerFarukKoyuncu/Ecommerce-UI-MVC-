                                <a class="dropdown-item" asp-area="Admin" asp-controller="Admin" asp-action="Profile" method="get" ><i class="ti-user m-r-5 m-l-5"></i> My Profile</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" href="/Admin/User/Logout"><i class="fa fa-power-off m-r-5 m-l-5"></i> Logout</a>
